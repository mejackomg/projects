{
  "name": "react-redux-starter-kit",
  "version": "0.0.1",
  "description": "Yet another React/Redux Starter Kit",
  "scripts": {
    "dev": "concurrent --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod",
    "start-dev-api": "better-npm-run start-dev-api",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "lint": "eslint src; exit 0",
    "clean": "rimraf dist",
    "dev-win": "SET NODE_ENV=development&&babel-node server.js",
    "build-win": "rimraf dist && SET NODE_ENV=production&& webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start-win": "SET NODE_ENV=production&&SET PORT=80&&babel-node server.js"
  },
  "betterScripts": {
    "start-dev": {
      "command": "babel-node server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-prod": {
      "command": "babel-node server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "babel-node api.js",
      "env": {
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "babel-node api.js",
      "env": {
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    }
  },
